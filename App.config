<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <appSettings>

    <!--General configuration-->
    <add key="BaseUrl" value="https://www.google.com/" />
    <add key="Username" value="yourname" />
    <add key="Password" value="Password" />

    <!--Enable or Disable Remote Browser-->
    <add key="RemoteBrowser" value="true" />

    <!--Firefox/Chrome-->
    <add key="Browser" value="Chrome" />

    <!--Mac/Windows/Linux-->
    <add key="Platform" value="Mac" />

    <!--Selenium Grid configuration-->
    <add key="UseSeleniumGrid" value="false" />
    <add key="GridHubUrl" value="http://localhost:4444/wd/hub" />

    <!--SauceLabs configuration-->
    <add key="UseSauceLabs" value="false" />
    <add key="SauceLabsUsername" value="yourname" />
    <add key="SauceLabsAccessKey" value="" />
    <add key="SauceLabsHubUrl" value="http://ondemand.saucelabs.com:80/wd/hub" />

    <!--Browserstack configuration-->
    <add key="UseBrowserstack" value="false" />
    <add key="BrowserStackUsername" value="yourname" />
    <add key="BrowserStackAccessKey" value="" />
    <add key="BrowserStackHubUrl" value="http://hub-cloud.browserstack.com/wd/hub/" />

  </appSettings>
</configuration>